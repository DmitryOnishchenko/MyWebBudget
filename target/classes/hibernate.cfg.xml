<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-configuration PUBLIC
        "-//Hibernate/Hibernate Configuration DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
	<session-factory>
		<property name="dialect">org.hibernate.dialect.MySQLDialect</property>
		<property name="show_sql">false</property>
		<mapping resource="com/electdead/webbudget/model/User.hbm.xml"/>
		<mapping resource="com/electdead/webbudget/model/Account.hbm.xml"/>
		<mapping resource="com/electdead/webbudget/model/Transaction.hbm.xml"/>
		<mapping resource="com/electdead/webbudget/model/Category.hbm.xml"/>
	</session-factory>
</hibernate-configuration>